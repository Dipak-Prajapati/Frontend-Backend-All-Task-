	<!-- c:set tag  -->
	<c:set var="i" value="22" scope="page" ></c:set>
	
	<h1>This is c:out </h1>
	<c:out value="${i}">Hiiiii</c:out>

	<!-- c:import  -->
	<c:import url="index.jsp" ></c:import>
</body>
</html>

