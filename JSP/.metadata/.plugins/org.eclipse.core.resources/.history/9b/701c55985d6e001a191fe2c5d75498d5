package tag;

import javax.servlet.jsp.tagext.TagSupport;

public class MyHandler extends TagSupport
{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private int number;
	private int power;
	
	public void setNumber(int number) {
		this.number = number;
	}
	public void setPower(int power) {
		this.power = power;
	}
	
	
	
}
